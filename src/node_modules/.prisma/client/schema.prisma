datasource db {
  provider = "postgresql"
  url = "postgresql://postgres:postgres@localhost:5443/estudy-db"
}

generator client {
  provider = "prisma-client-js"
  output = "../node_modules/.prisma/client"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}